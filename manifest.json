{
  "name": "__MSG_appName__",
  "version": "1.0.4",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "icons": {
    "48": "img/icon-48x48.png",
    "128": "img/icon-128x128.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "host_permissions": [
    "*://*.zoom.us/*",
    "*://*.zoomgov.com/*"
  ],
  "action": {
    "default_title": "__MSG_actionTitle__"
  },
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "zoom-redirect",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "manifest_version": 3
}
